<h2>Import Users</h2>

<%= form_tag import_users_path, multipart: true do %>
  <%= file_field_tag :file %>
  <%= submit_tag "Import" %>
<% end %>


<table>
  <tr>
    <th>First Name</th>
    <th>count</th>
  </tr>
     <% @users.each do |student| %>
    <tr>
      <td><%= student.first_name %></td>
      <td><%= student.count %></td>
    </tr>
  <% end %>
</table>